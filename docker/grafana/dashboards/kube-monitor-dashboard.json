{
  "dashboard": {
    "__inputs": [
      {
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "description": "",
        "type": "datasource",
        "pluginId": "prometheus",
        "pluginName": "Prometheus"
      },
      {
        "name": "DS_LOKI",
        "label": "Loki",
        "description": "",
        "type": "datasource",
        "pluginId": "loki",
        "pluginName": "Loki"
      },
      {
        "name": "DS_JAEGER",
        "label": "Jaeger",
        "description": "",
        "type": "datasource",
        "pluginId": "jaeger",
        "pluginName": "Jaeger"
      }
    ],
    "timezone": "browser",
    "title": "Kube Monitor Probe Kit",
    "tags": ["spring-boot", "tracing"],
    "schemaVersion": 39,
    "version": 6,
    "time": { "from": "now-1h", "to": "now" },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Application Health",
        "type": "stat",
        "datasource": "${DS_PROMETHEUS}",
        "targets": [
          {
            "refId": "A",
            "expr": "max(up{job=~\"spring-app|.*spring.*\"})",
            "datasource": "${DS_PROMETHEUS}"
          }
        ],
        "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Log Count / Minute",
        "type": "stat",
        "datasource": "${DS_LOKI}",
        "targets": [
          {
            "refId": "A",
            "expr": "sum(count_over_time({job=~\"spring-app|.*spring.*\"}[1m]))",
            "datasource": "${DS_LOKI}"
          }
        ],
        "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 5,
        "title": "Request Rate",
        "type": "stat",
        "datasource": "${DS_PROMETHEUS}",
        "targets": [
          {
            "refId": "A",
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-app|.*spring.*\"}[1m]))",
            "datasource": "${DS_PROMETHEUS}"
          }
        ],
        "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } },
        "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 6,
        "title": "P95 Response Time",
        "type": "stat",
        "datasource": "${DS_PROMETHEUS}",
        "targets": [
          {
            "refId": "A",
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-app|.*spring.*\"}[5m])) by (le))",
            "datasource": "${DS_PROMETHEUS}"
          }
        ],
        "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } },
        "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 3,
        "title": "HTTP Request Rate (req/s)",
        "type": "timeseries",
        "datasource": "${DS_PROMETHEUS}",
        "targets": [
          {
            "refId": "A",
            "expr": "sum by (method, status) (rate(http_server_requests_seconds_count{job=~\"spring-app|.*spring.*\"}[1m]))",
            "datasource": "${DS_PROMETHEUS}"
          }
        ],
        "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
        "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 7,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "datasource": "${DS_PROMETHEUS}",
        "targets": [
          {
            "refId": "A",
            "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-app|.*spring.*\"}[5m])) by (le))",
            "legendFormat": "P50",
            "datasource": "${DS_PROMETHEUS}"
          },
          {
            "refId": "B",
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-app|.*spring.*\"}[5m])) by (le))",
            "legendFormat": "P95",
            "datasource": "${DS_PROMETHEUS}"
          },
          {
            "refId": "C",
            "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-app|.*spring.*\"}[5m])) by (le))",
            "legendFormat": "P99",
            "datasource": "${DS_PROMETHEUS}"
          }
        ],
        "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
        "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 8,
        "title": "Recent Traces",
        "type": "traces",
        "datasource": "${DS_JAEGER}",
        "targets": [
          {
            "refId": "A",
            "queryType": "search",
            "service": "spring-app",
            "operation": "",
            "tags": "",
            "minDuration": "",
            "maxDuration": "",
            "limit": 20,
            "datasource": "${DS_JAEGER}"
          }
        ],
        "gridPos": { "h": 10, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 9,
        "title": "Trace Analysis",
        "type": "table",
        "datasource": "${DS_JAEGER}",
        "targets": [
          {
            "refId": "A",
            "queryType": "search",
            "service": "spring-app",
            "operation": "",
            "tags": "",
            "minDuration": "",
            "maxDuration": "",
            "limit": 10,
            "datasource": "${DS_JAEGER}"
          }
        ],
        "options": {
          "showHeader": true,
          "footer": { "show": false, "reducer": ["sum"] }
        },
        "gridPos": { "h": 10, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 4,
        "title": "Recent Logs",
        "type": "logs",
        "datasource": "${DS_LOKI}",
        "targets": [
          {
            "refId": "A",
            "expr": "{job=~\"spring-app|.*spring.*\"}",
            "datasource": "${DS_LOKI}"
          }
        ],
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 26 }
      }
    ]
  },
  "folderUid": "general",
  "message": "Dashboard updated",
  "overwrite": true
}