{
  "id": null,
  "uid": "kmpk-obsv",
  "title": "kube-monitor-probe-kit \u2013 Observability",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "kube-monitor-probe-kit",
    "otel",
    "spring",
    "prometheus",
    "loki",
    "jaeger"
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "DS_PROM",
        "type": "datasource",
        "query": "Prometheus",
        "label": "Prometheus"
      },
      {
        "name": "DS_LOKI",
        "type": "datasource",
        "query": "Loki",
        "label": "Loki"
      },
      {
        "name": "DS_JAEGER",
        "type": "datasource",
        "query": "Jaeger",
        "label": "Jaeger"
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "RPS",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": "${DS_PROM}",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\\\"kube-monitor-probe-kit\\\"}[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Hata Oran\u0131 (5xx, %)",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 0.05
              },
              {
                "color": "red",
                "value": 0.1
              }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\\\"kube-monitor-probe-kit\\\",status=~\\\"5..\\\"}[5m])) / sum(rate(http_server_requests_seconds_count{job=\\\"kube-monitor-probe-kit\\\"}[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "p95 Gecikme (s)",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": "${DS_PROM}",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{job=\\\"kube-monitor-probe-kit\\\"}[5m])))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "\u0130stek H\u0131z\u0131 \u2013 2xx/4xx/5xx",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "datasource": "${DS_PROM}",
      "lines": true,
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\\\"kube-monitor-probe-kit\\\",status=~\\\"2..\\\"}[5m]))",
          "legendFormat": "2xx",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\\\"kube-monitor-probe-kit\\\",status=~\\\"4..\\\"}[5m]))",
          "legendFormat": "4xx",
          "refId": "B"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\\\"kube-monitor-probe-kit\\\",status=~\\\"5..\\\"}[5m]))",
          "legendFormat": "5xx",
          "refId": "C"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Gecikme \u2013 p50/p95/p99 (s)",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "datasource": "${DS_PROM}",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_server_requests_seconds_bucket{job=\\\"kube-monitor-probe-kit\\\"}[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{job=\\\"kube-monitor-probe-kit\\\"}[5m])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{job=\\\"kube-monitor-probe-kit\\\"}[5m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Custom Operations \u2013 rate(custom_operations_total)",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 8,
        "h": 7
      },
      "datasource": "${DS_PROM}",
      "targets": [
        {
          "expr": "sum(rate(custom_operations_total{job=\\\"kube-monitor-probe-kit\\\"}[5m]))",
          "legendFormat": "ops/s",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "CPU ve Uptime",
      "gridPos": {
        "x": 8,
        "y": 12,
        "w": 8,
        "h": 7
      },
      "datasource": "${DS_PROM}",
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=\\\"kube-monitor-probe-kit\\\"}[1m])",
          "legendFormat": "cpu sec/s",
          "refId": "A"
        },
        {
          "expr": "time() - process_start_time_seconds{job=\\\"kube-monitor-probe-kit\\\"}",
          "legendFormat": "uptime",
          "refId": "B"
        }
      ]
    },
    {
      "type": "graph",
      "title": "JVM Heap Used/Committed/Max (MB)",
      "gridPos": {
        "x": 16,
        "y": 12,
        "w": 8,
        "h": 7
      },
      "datasource": "${DS_PROM}",
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{job=\\\"kube-monitor-probe-kit\\\",area=\\\"heap\\\"})/1024/1024",
          "legendFormat": "used",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_memory_committed_bytes{job=\\\"kube-monitor-probe-kit\\\",area=\\\"heap\\\"})/1024/1024",
          "legendFormat": "committed",
          "refId": "B"
        },
        {
          "expr": "sum(jvm_memory_max_bytes{job=\\\"kube-monitor-probe-kit\\\",area=\\\"heap\\\"})/1024/1024",
          "legendFormat": "max",
          "refId": "C"
        }
      ]
    },
    {
      "type": "logs",
      "title": "Logs (Loki) \u2013 kube-monitor-probe-kit",
      "gridPos": {
        "x": 0,
        "y": 19,
        "w": 16,
        "h": 10
      },
      "datasource": "${DS_LOKI}",
      "targets": [
        {
          "expr": "{job=\\\"kube-monitor-probe-kit\\\"}",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true
      }
    },
    {
      "type": "jaeger",
      "title": "Traces (Jaeger) \u2013 kube-monitor-probe-kit",
      "gridPos": {
        "x": 16,
        "y": 19,
        "w": 8,
        "h": 10
      },
      "datasource": "${DS_JAEGER}",
      "options": {
        "query": {
          "service": "kube-monitor-probe-kit",
          "limit": 20,
          "maxDuration": null,
          "minDuration": null,
          "tags": ""
        }
      }
    }
  ]
}